generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Income {
  id       Int       @id @default(autoincrement())
  value    Int
  date     DateTime? @default(now())
  comment  String?
  category String?
}

model Expenditure {
  id       Int       @id @default(autoincrement())
  value    Int
  date     DateTime? @default(now())
  comment  String?
  category String?
}
